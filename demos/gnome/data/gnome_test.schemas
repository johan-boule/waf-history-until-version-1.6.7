<gconfschemafile>
  <schemalist>

    <schema>
      <key>/schemas/apps/gnome_test/enable_pcskr_beeping</key>
      <applyto>/apps/gnome_test/enable_pcskr_beeping</applyto>
      <owner>gnome_test</owner>
      <type>bool</type>
      <default>true</default>
      <locale name="C">
        <short>If the PC speaker (beeping) should be used</short>
        <long>If the PC speaker should be used to beep when the power is low, or inhibit requests have stopped the policy action.</long>
      </locale>
    </schema>

  </schemalist>
</gconfschemafile>
